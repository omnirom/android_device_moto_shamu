[access "refs/heads/*"]
	label-Code-Review = -2..+2 group Shamu Maintainers
	label-Verified = -1..+1 group Shamu Maintainers
	submit = group Shamu Maintainers
	push = +force group Shamu Maintainers
	pushMerge = group Shamu Maintainers
	pushTag = +force group Shamu Maintainers
	pushSignedTag = group Shamu Maintainers
[access "refs/for/refs/heads/*"]
	pushMerge = group Shamu Maintainers
